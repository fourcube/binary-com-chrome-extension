{
  "name": "Binary.com Helper",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Trading helpers",
  "homepage_url": "https://grieger.biz",
  "content_scripts": [{
    "js": [
      "background.js"
    ],
    "matches": [
      "*://www.binary.com/*"
    ]
  }],
  "web_accessible_resources": [
    "**/*.*"
  ],
  "permissions": [
    "https://www.binary.com/*",
    "tabs"
  ]
}